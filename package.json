{
  "name": "snoots",
  "version": "1.0.0-dev.6",
  "description": "A modern, fully-featured, strongly-typed reddit api.",
  "module": "./dist/index.mjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "prepare": "is-ci || husky install",

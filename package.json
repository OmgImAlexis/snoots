    "prepublishOnly": "npm run clean && npm run lint && npm run build"
  },
  "dependencies": {
    "camelcase": "^6.2.0",
    "got": "^11.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
